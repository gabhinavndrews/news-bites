{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport newsCard from './newsCard.vue';\nexport default {\n  name: 'newsSection',\n  components: {\n    newsCard\n  },\n\n  data() {\n    return {\n      search: '',\n      result: 'All'\n    };\n  },\n\n  props: {\n    news: Array\n  },\n  methods: {\n    onClick() {\n      console.log(this.filter);\n    },\n\n    resultFn() {\n      if (this.search != '') {\n        this.result = this.search;\n        return this.result;\n      } else {\n        this.result = 'All';\n        return this.result;\n      }\n    }\n\n  },\n  computed: {\n    filteredNews() {\n      return this.news.filter(news => news.genre.toLowerCase().includes(this.search.toLowerCase()));\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAwBA,OAAOA,QAAP,MAAqB,gBAArB;AAIA,eAAe;EACXC,IAAI,EAAE,aADK;EAEXC,UAAS,EAAI;IACTF;EADS,CAFF;;EAKXG,IAAI,GAAG;IACL,OAAO;MACLC,MAAM,EAAE,EADH;MAELC,MAAM,EAAE;IAFH,CAAP;EAID,CAVU;;EAWXC,KAAK,EAAC;IACFC,IAAI,EAAEC;EADJ,CAXK;EAcXC,OAAO,EAAE;IACPC,OAAO,GAAG;MACRC,OAAO,CAACC,GAAR,CAAY,KAAKC,MAAjB;IACD,CAHM;;IAIPC,QAAQ,GAAG;MACT,IAAG,KAAKV,MAAL,IAAe,EAAlB,EAAsB;QACpB,KAAKC,MAAL,GAAc,KAAKD,MAAnB;QACA,OAAO,KAAKC,MAAZ;MACD,CAHD,MAGM;QACJ,KAAKA,MAAL,GAAc,KAAd;QACA,OAAO,KAAKA,MAAZ;MACF;IACF;;EAZO,CAdE;EA4BXU,QAAQ,EAAE;IACRC,YAAY,GAAE;MACZ,OAAO,KAAKT,IAAL,CAAUM,MAAV,CAAiBN,IAAG,IAAKA,IAAI,CAACU,KAAL,CAAWC,WAAX,GAAyBC,QAAzB,CAAkC,KAAKf,MAAL,CAAYc,WAAZ,EAAlC,CAAzB,CAAP;IACF;;EAHQ;AA5BC,CAAf","names":["newsCard","name","components","data","search","result","props","news","Array","methods","onClick","console","log","filter","resultFn","computed","filteredNews","genre","toLowerCase","includes"],"sourceRoot":"","sources":["C:\\Users\\gabhi\\OneDrive\\Desktop\\vue project\\newsbites\\abhinavandrewsgiduturi_vue\\src\\components\\newsSection.vue"],"sourcesContent":["<template>\r\n  <v-container class=\"newsSection\">\r\n    <form class=\"search\">\r\n      <input class=\"search-box\" type=\"text\" v-model=\"search\" placeholder=\"Search keywords..\">\r\n      <select class=\"filter\"  type=\"text\" v-model=\"search\">\r\n        <option disabled value=\"\">Please select a genre</option>\r\n        <option value=\"\">All</option>\r\n        <option>sports</option>\r\n        <option>business</option>\r\n        <option>local</option>\r\n        <option>movies</option>\r\n        <option>international</option>\r\n      </select>\r\n      <h1>Showing results for: {{resultFn()}}</h1>\r\n    </form>\r\n    <v-row>\r\n    <v-col v-for=\" newsArticle in filteredNews\" :key=\"newsArticle.id\" >\r\n      <newsCard :news=\"newsArticle\" :filter = \"this.filter\" @onClick=\"onClick()\"  />\r\n    </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport newsCard from './newsCard.vue'\r\n\r\n\r\n\r\nexport default {\r\n    name: 'newsSection',\r\n    components : {\r\n        newsCard\r\n    },\r\n    data() {\r\n      return {\r\n        search: '',\r\n        result: 'All'\r\n      }\r\n    },\r\n    props:{\r\n        news: Array\r\n    },\r\n    methods: {\r\n      onClick() { \r\n        console.log(this.filter);\r\n      },\r\n      resultFn() {\r\n        if(this.search != '') {\r\n          this.result = this.search;\r\n          return this.result\r\n        }else {\r\n          this.result = 'All'\r\n          return this.result\r\n        }\r\n      }\r\n    },\r\n    computed: {\r\n      filteredNews(){\r\n        return this.news.filter(news => news.genre.toLowerCase().includes(this.search.toLowerCase()))\r\n      }\r\n    }\r\n    \r\n}\r\n</script>\r\n\r\n<style>\r\n    .newsSection {\r\n        display: block;\r\n        background-color: #fdfcdc;\r\n        border: 2px solid black;\r\n        padding: 10px;\r\n        margin: 10px;\r\n        text-align: center;\r\n    }\r\n\r\n    .search {\r\n      display: block;\r\n      margin: 10px;\r\n      float: center;\r\n    }\r\n\r\n    .search-box {\r\n      width: 200px;\r\n      height: 30px;\r\n  